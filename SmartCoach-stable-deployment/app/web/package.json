{
  "name": "smart-coach",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build --prod -- --stats-json && webpack-bundle-analyzer dist/smartCoach/stats.json",
    "doc": "npx compodoc -p tsconfig.app.json --theme laravel --name Documentation --hideGenerator  --customLogo src/assets/icon.png --customFavicon src/assets/icon.png",
    "buildMobile": "ng build --base-href '' --output-path ../mobileProd/www/",
    "buildProdMobile": "ng build --configuration=production --base-href '' --output-path ../mobileProd/www/",
    "buildFrontendForDev": "ng build --base-href https://smart-coach-development-hub.netlify.app/frontendmasterbuild/ --output-path  ../../../Smart-Coach-Development-Hub/frontendMasterBuild/",
    "buildFrontendForDevToTest": "ng build  --base-href https://smart-coach-development-hub.netlify.app/frontendtestingbuild/ --output-path  ../../../Smart-Coach-Development-Hub/frontendTestingBuild/",
    "buildProdFrontendForDevToTest": "ng build --prod --base-href https://smart-coach-development-hub.netlify.app/frontendtestingbuild/ --output-path  ../../../Smart-Coach-Development-Hub/frontendTestingBuild/",
    "buildDocumentationForDev": "npx compodoc -p tsconfig.app.json --theme laravel --name Documentation --hideGenerator --customLogo src/assets/icon.png --customFavicon src/assets/icon.png --output ../../../Smart-Coach-Development-Hub/frontendDocumentation",
    "buildTestCoverageForDev": "ng test --watch=false --code-coverage --karmaConfig=karmaDevEnv.conf.js",
    "pushChangesToDevHub": "cd ../../../Smart-Coach-Development-Hub && git add .  && git commit -am. && git push --force ",
    "updateDevEnvironment": "npm run buildFrontendForDev && npm run buildDocumentationForDev && npm run buildTestCoverageForDev && npm run pushChangesToDevHub ",
    "deployTestBuildToDev": "npm run buildFrontendForDevToTest && npm run pushChangesToDevHub",
    "deployProdTestBuildToDev": "npm run buildProdFrontendForDevToTest && npm run pushChangesToDevHub",
    "buildProdBundle": "ng build --prod --output-path ../../bundle/app --base-href /app/",
    "pushChangesToProd": "cd ../../../smart-coach-production && git add . && git commit -am. && git push --force",
    "moveBundleContentsOver": "cd ../../ && move bundle ../smart-coach-production/bundle",
    "moveBundleContentsBack": "cd ../../ && move ../smart-coach-production/bundle ./bundle",
    "deployProdBundle": "npm run buildProdBundle && npm run moveBundleContentsOver && npm run pushChangesToProd && npm run moveBundleContentsBack",
    "switchToFirebaseDevCLI": "firebase use testing",
    "switchToFirebaseProdCLI": "firebase use prodction",
    "testWithCoverage": "ng test --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "-": "0.0.1",
    "@angular-devkit/schematics": "^14.1.0",
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/fire": "^7.4.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/pwa": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@stripe/stripe-js": "^1.27.0",
    "@types/facebook-js-sdk": "^3.3.1",
    "angular-shepherd": "^14.0.0",
    "bootstrap": "^5.2.0",
    "chart.js": "^3.8.2",
    "chartjs-plugin-zoom": "^1.2.1",
    "export-to-csv": "^0.2.1",
    "firebase": "^9.9.1",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.6.0",
    "firebase-tools": "^11.6.0",
    "g": "^2.0.1",
    "ng-particles": "^3.1.4",
    "ng2-charts": "^4.0.0",
    "ngentest": "^1.4.1",
    "prettier": "^2.7.1",
    "rxjs": "~7.5.6",
    "swiper": "^8.3.1",
    "tslib": "^2.0.0",
    "tsparticles": "^2.2.0",
    "tsparticles-engine": "^2.2.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/crypto-js": "^3.1.47",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "@types/stripe-checkout": "^1.0.4",
    "@types/stripe-v3": "^3.1.26",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-spec": "^2.0.0",
    "protractor": "^7.0.0",
    "scuri": "^1.0.3",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
